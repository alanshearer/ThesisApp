<UserControl x:Class="_Scavi.CustomPushpin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="140" d:DesignWidth="140">

    <Grid x:Name="LayoutRoot" Background="Transparent" Margin="0,0,0,10">
        <Image x:Name="PushpinImage" HorizontalAlignment="Left" Height="140" VerticalAlignment="Top" Width="140" Margin="0,0,0,0"/>

    </Grid>
</UserControl>